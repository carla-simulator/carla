                    "                        == road[0]\n",
                    "                    ):\n",
                    "                        different_road_distance = int(distance_to_junc / 3)\n",
                    "                    elif (\n",
                    "                        str(\n",
                    "                            actor_waypoint.previous(int(distance_to_junc / 3))[\n",
                    "                                0\n",
                    "                            ].road_id\n",
                    "                        )\n",
                    "                        == road[0]\n",
                    "                    ):\n",
                    "                        different_road_distance = (\n",
                    "                            int(distance_to_junc / 3) * -1\n",
                    "                        )  # negative bc. look back from actor\n",
                    "                    else:\n",
