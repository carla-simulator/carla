                    "            yaw = 1\n",
                    "    else:\n",
                    "        ego_transform = ego_vehicle.get_transform()\n",
                    "        ego_rotation = ego_transform.rotation\n",
                    "        yaw = ego_rotation.yaw\n",
                    "\n",
                    "    return key_value_pairs, lanes_all, junction_roads, yaw"
                ]
            },
