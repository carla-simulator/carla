                    "def angle_between_vectors(a, b):\n",
                    "    dot = a.x * b.x + a.y * b.y\n",
                    "    mag_a = math.sqrt(a.x**2 + a.y**2)\n",
                    "    mag_b = math.sqrt(b.x**2 + b.y**2)\n",
                    "    cos_theta = dot / (mag_a * mag_b)\n",
                    "    theta = math.acos(cos_theta)\n",
                    "    return math.degrees(theta)"
                ]
            },
