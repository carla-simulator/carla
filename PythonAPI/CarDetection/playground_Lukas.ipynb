                    "                if car.id == ego_vehicle.id:\n",
                    "                    matrix[str(ego_vehicle_road_id) + \"_\" + str(other_car_lane_id)][\n",
                    "                        col\n",
                    "                    ] = 1\n",
                    "                else:\n",
                    "                    try:\n",
                    "                        matrix[str(ego_vehicle_road_id) + \"_\" + str(other_car_lane_id)][\n",
                    "                            col\n",
                    "                        ] = 2\n",
                    "                    except:\n",
                    "                        pass\n",
                    "\n",
                    "    return matrix, surrounding_cars_on_highway_entryExit"
                ]
            },
